<?xml version="1.0"?>
<mvc>
  <profile name="erp-order-admin" package="com.yushanginfo.erp.order.admin.web.action">
    <url path="/" suffix=""  style="seo" />
    <interceptors>
      <interceptor name="web.Interceptor.hibernate"/>
      <interceptor name="web.Interceptor.security"/>
    </interceptors>
  </profile>

  <profile name="erp-order-ws" package="com.yushanginfo.erp.order.admin.web.ws">
    <action suffix="Ws" defaultMethod="index" />
    <url path="/" suffix=""  style="plur-seo" />
    <interceptors>
      <interceptor name="web.Interceptor.hibernate"/>
      <interceptor name="web.Interceptor.security"/>
    </interceptors>
  </profile>
</mvc>
